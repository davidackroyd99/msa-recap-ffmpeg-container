{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "audioBlob",
      "type": "blob",
      "dataType": "binary",
      "path": "recordings/{Audio.FileName}",
      "connection": "MyStorageConnectionAppSetting",
      "direction": "in"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "dataType": "binary",
      "path": "recordings/{Transcript.FileName}",
      "connection": "MyStorageConnectionAppSetting",
      "direction": "out"
    }
  ]
}
